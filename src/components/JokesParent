import jokes from "./jokes"
function JokeReveal(prop){
    return(
        <div>
            <p>
            {prop.answer}
            </p>
        </div>
    )
}
function JokesParent(prop){
    return(
        <div>
        <jokes/>
        <button onclick="jokeReveal(prop)"> Who is it? </button>
    )
}

export JokesParent;